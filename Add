import java.util.Scanner;
class Add 
{
	public static void main(String[] args) 
	{
		Scanner scn=new Scanner(System.in);
		System.out.println("Enter a num: ");
		int n = scn.nextInt();
		int sum = 0;
		do
		{
		while (n>0){
			sum+=n%10;
			n/=10;
		}
		n=sum;
		sum=0;
		System.out.println(n);
		}while(n>9);
	}
}
